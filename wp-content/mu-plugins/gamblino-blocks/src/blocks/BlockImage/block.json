{
	"apiVersion": 2,
	"name": "gamblino-block/image",
	"version": "1.0.0",
	"title": "Block Image",
	"category": "gamblino",
	"icon": "smiley",
	"description": "Just a simple Gamblino Image Block",
	"keywords": ["gamblino", "image", "preview"],
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},

	"textdomain": "block-gamblino",
	"attributes": {
		"id": {
			"type": "number"
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"alt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"imageDuplication": {
			"type": "array",
			"default": [{ "count": 1 }]
		},
		"countImageFilterOpacity": {
			"type": "number",
			"default": 0
		},
		"isOrientedImage": {
			"type": "boolean",
			"default": false
		},
		"imageOpacityLevel": {
			"type": "number",
			"default": 1
		},
		"imageDimension": {
			"type": "string",
			"default": "100%"
		},
		"imageDimensionWidth": {
			"type": "number"
		},
		"imageDimensionHeight": {
			"type": "number"
		},
		"imageSizeVariant": {
			"type": "string",
			"default": "primary"
		},
		"displayPosition": {
			"type": "array",
			"default": ["Inherit", "Static", "Relative", "Absolute"]
		},
		"positionType": {
			"type": "string",
			"default": "static"
		},
		"positionValue": {
			"type": "object",
			"default": {
				"top": 0,
				"left": 0,
				"right": 0,
				"bottom": 0
			}
		},
		"marginUnitArray": {
			"type": "array",
			"default": ["px", "em", "rem"]
		},
		"marginUnit": {
			"type": "string",
			"default": "px"
		},
		"margin": {
			"type": "object",
			"default": {
				"top": 0,
				"right": 0,
				"bottom": 0,
				"left": 0
			}
		},
		"marginAuto": {
			"type": "string",
			"default": {
				"margin": "auto"
			}
		},
		"isResetMargin": {
			"type": "boolean",
			"default": false
		},
		"hrefLinkTarget": {
			"type": "string",
			"default": ""
		},
		"isLinkToolbarButtonOpen": {
			"type": "boolean",
			"default": false
		},
		"toggleNewTab": {
			"type": "boolean",
			"default": false
		},
		"isToggledSEO": {
			"type": "boolean",
			"default": false
		},
		"isNewTabLinkURLToggled": {
			"type": "boolean",
			"default": false
		},
		"isSponsoredToggled": {
			"type": "boolean",
			"default": false
		},
		"isUGCToggled": {
			"type": "boolean",
			"default": false
		},
		"isFollowToggled": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": "file:../../../build/block-image.js",
	"editorStyle": "file:../../../build/block-image.css",
	"style": "file:../../../build/style-block-image.css"
}
