{
	"apiVersion": 2,
	"name": "gamblino-block/general-information",
	"version": "1.0.0",
	"title": "Block Gamblino General information",
	"category": "gamblino",
	"icon": "smiley",
	"description": "Gamblino Info Bonus Review Block",
	"keywords": [
		"gamblino",
		"bonus",
		"info",
		"review",
		"about us",
		"advantages",
		"limitations"
	],
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"id": {
			"type": "number",
			"default": 0
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src",
			"default": ""
		},
		"alt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"author": {
			"type": "string"
		},
		"postLastUpdated": {
			"type": "string"
		},
		"topicName": {
			"type": "string"
		},
		"experiences": {
			"type": "string"
		},
		"tableTitle": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"tableTitleColorPicker": {
			"type": "string"
		},
		"tableTitleAlignment": {
			"type": "string",
			"default": "left"
		},
		"tableBodyData": {
			"type": "array",
			"source": "query",
			"selector": "tbody tr",
			"default": [],
			"query": {
				"textColumnOne": {
					"type": "string",
					"source": "html",
					"selector": "p"
				},
				"textColumnTwo": {
					"type": "string",
					"source": "html",
					"selector": "div"
				}
			}
		}
	},
	"textdomain": "block-gamblino",
	"editorScript": "file:../../../build/block-general-information.js",
	"editorStyle": "file:../../../build/block-general-information.css",
	"style": "file:../../../build/style-block-general-information.css"
}
