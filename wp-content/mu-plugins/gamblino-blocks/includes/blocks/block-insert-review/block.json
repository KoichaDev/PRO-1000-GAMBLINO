{
	"apiVersion": 2,
	"name": "gamblino-block/insert-review",
	"version": "1.0.0",
	"title": "Block Insert Review",
	"category": "gamblino",
	"icon": "smiley",
	"description": "Gamblino Info Bonus Review Block",
	"keywords": ["gamblino", "bonus", "info", "review", "insert"],
	"supports": {
		"html": false,
		"classname": false
	},
	"textdomain": "block-gamblino",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"description": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"lists": {
			"type": "array",
			"selector": ".block-insert-review__lists li",
			"source": "query",
			"default": [],
			"query": {
				"content": {
					"type": "string",
					"source": "html",
					"selector": "p"
				}
			}
		},
		"buttonText": {
			"type": "string",
			"source": "html",
			"selector": "a"
		},
		"isShadowMenuOpen": {
			"type": "boolean",
			"default": false
		},
		"shadowOpacity": {
			"type": "number",
			"default": 30
		},
		"isBorderRadiusMenuOpen": {
			"type": "boolean",
			"default": false
		},
		"buttonIsClickedLinkSides": {
			"type": "boolean",
			"default": false
		},
		"buttonPadding": {
			"type": "number",
			"default": 12
		},
		"paddingSelectUnit": {
			"type": "string",
			"default": "px"
		},
		"buttonPaddingHorizontal": {
			"type": "number",
			"default": 12
		},
		"paddingHorizontalSelectUnit": {
			"type": "string",
			"default": "px"
		},
		"buttonPaddingVertical": {
			"type": "number",
			"default": 12
		},
		"paddingVerticalSelectUnit": {
			"type": "string",
			"default": "px"
		},
		"buttonBorderRadius": {
			"type": "number",
			"default": 1
		},
		"buttonBackgroundColor": {
			"type": "string",
			"default": "#6FCF97"
		},
		"buttonColor": {
			"type": "string",
			"default": "#fff"
		}
	},
	"editorScript": "file:../../../build/block-insert-review.js",
	"editorStyle": "file:../../../build/block-insert-review.css",
	"style": "file:../../../build/style-block-insert-review.css"
}
